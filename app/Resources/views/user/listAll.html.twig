{% extends 'base.html.twig' %}
{% block title %}list all {% endblock %}
{% block body %}
    <h1>Users</h1>
    <ul>
        <li>---------------</li>
        {% for user in users %}
            <li>{{ user.id }}</li>
            <li>{{ user.username }}</li>
            <li>{{ user.lastname }}, {{ user.firstname }}</li>
            <li>{{ user.email }}</li>
            <li>---------------</li>
        {% endfor %}
    </ul>
{% endblock %}